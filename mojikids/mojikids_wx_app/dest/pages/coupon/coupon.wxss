/**coupon.wxss**/
.pagemain {
  padding: 40rpx 15px; }
  .pagemain .card {
    box-shadow: 0px 0px 10px 3px rgba(127, 127, 127, 0.1);
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-left-radius: 8px;
    border-bottom-right-radius: 8px;
    min-height: 358rpx;
    background: #ffb628;
    color: #d2730c;
    position: relative;
    margin-bottom: 40rpx; }
    .pagemain .card .img {
      position: absolute;
      top: 0;
      left: 0;
      z-index: 1; }
    .pagemain .card .top {
      position: relative;
      top: 0;
      z-index: 2; }
      .pagemain .card .top .top_left {
        padding-top: 30rpx;
        padding-left: 44rpx; }
        .pagemain .card .top .top_left .price {
          color: #fff;
          font-size: 72rpx; }
        .pagemain .card .top .top_left .des {
          font-size: 32rpx; }
    .pagemain .card .bottom {
      font-size: 24rpx;
      padding: 30rpx 30rpx 30rpx 30rpx; }
      .pagemain .card .bottom .inn_des {
        margin-bottom: 16rpx; }
        .pagemain .card .bottom .inn_des:last-child {
          margin-bottom: 0px; }
      .pagemain .card .bottom .des_title {
        min-width: 120rpx;
        word-break: break-all; }
      .pagemain .card .bottom .des_value {
        word-break: break-all;
